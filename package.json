{
  "name": "spotify",
  "displayName": "Spotify",
  "version": "0.0.0",
  "description": "example spotify plugin for dotBC",
  "publisher": "dotBC",
  "main": "index.js",
  "capabilities": {
    "search": {
      "name": "Spotify",
      "description": "Look up artists on Spotify.",
      "imageUrl": "public/images/icon-spotify.svg",
      "searchImageUrl": "public/images/icon-spotify-button.svg",
      "supportedSearchTypes": [
        "artist",
        "work"
      ],
      "commands": [
        "search",
        "getDetails"
      ]
    }
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cardstack-extension-manager": "git+ssh://git@github.com/mateodelnorte/cardstack-extension-manager.git",
    "debug": "^2.2.0",
    "lodash": "^4.15.0",
    "superagent": "^2.2.0"
  }
}
